@using LtInfo.Common.ModalDialog
@using LtInfo.Common.Mvc
@using ProjectFirma.Web.Common
@using ProjectFirma.Web.Models
@using ProjectFirmaModels.Models
@inherits ProjectFirma.Web.Views.ExcelUpload.ImportPnBudgetsExcelFile

<div class="validationError">
    @Html.ValidationSummary()
</div>

    <table style="width: 100%" class="summaryLayout">
        <tr>
            <td style="width: 350px">
               
                <p>
                    Upload an xlsx file.
                </p>
                <form method="POST" id="@ViewDataTyped.PnBudgetsUploadFormID" action="@ViewDataTyped.NewPnBudgetsExcelUploadUrl">
                <div>
                    @Html.EditorFor(x => x.FileResourceData)@Html.ValidationMessageFor(x => x.FileResourceData)
                    <span class="smallExplanationText">
                        Allowed Extensions: @Model.GetFileExtensions(x => x.FileResourceData)
                    </span>
                </div>
                </form>
            </td>

        </tr>
    </table>


